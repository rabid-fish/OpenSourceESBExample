<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:cxf="http://camel.apache.org/schema/cxf"
	xmlns:camel="http://camel.apache.org/schema/spring"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://camel.apache.org/schema/cxf http://camel.apache.org/schema/cxf/camel-cxf.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
">

	<context:component-scan base-package="com.github.rabid_fish.initiator" />
	
	<camelContext xmlns="http://camel.apache.org/schema/spring" id="mathOperationInitiatorCamelContext">

		<camel:endpoint id="mathOperationInputDirectEndpoint" uri="${mathOperationInputDirectEndpoint}" />
		<camel:endpoint id="mathOperationInputQueueEndpoint" uri="${mathOperationInputQueueEndpoint}" />

		<route id="mathOperationInputRoute">
			<camel:from ref="mathOperationInputDirectEndpoint" />
			<camel:to ref="mathOperationInputQueueEndpoint" />
		</route>

	</camelContext>

</beans>
